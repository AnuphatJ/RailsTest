<p id="notice"><%= notice %></p>

<h1>Listing Anuphats</h1>

<table>
  <thead>
    <tr>
      <th>Login</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @anuphats.each do |anuphat| %>
      <tr>
        <td><%= anuphat.login %></td>
        <td><%= link_to 'Show', anuphat %></td>
        <td><%= link_to 'Edit', edit_anuphat_path(anuphat) %></td>
        <td><%= link_to 'Destroy', anuphat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Anuphat', new_anuphat_path %>
